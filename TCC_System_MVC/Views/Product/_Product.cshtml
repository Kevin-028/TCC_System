@model TCC_System_Application.ArduinoService.ProductViewModel


<form  class="row g-3 needs-validation" novalidate>
    <div class="col-12">
        @Html.LabelFor(model => model.Name, new { @class = "form-label" }) :
        @Html.TextBoxFor(model => model.Name, new { @class = "form-control", required = "required" })
    </div>
    <div class="col-12">
        <button id="Save" class="btn btn-primary w-100 newProject" type="button">
            <span class="spinner-border spinner-border-sm visually-hidden" role="status" aria-hidden="true"></span>
            <span class="">Criar Peojeto</span>
        </button>
    </div>
</form>
